{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "crons": [{
        "path": "/api/v1/users/ticket-create",
        "schedule": "08 * * * *"
      }]
}
